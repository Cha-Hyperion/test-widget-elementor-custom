{
    "name": "czelements/charlenezybala",
    "description": "Add on pour Elementor, ajout de nouveaux widgets",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Charlene Zybala",
            "homepage": "https://charlenezybala.com",
            "email": "contact@charlenezyabala.com"
        }
    ],
    "require": {
        "johnpbloch/wordpress": "^5.6",
        "wpackagist-plugin/elementor": "^3.0",
        "wpackagist-theme/hello-elementor": "^2.3"
    },
    "require-dev": {
        "wpackagist-plugin/query-monitor": "^3.6"
    },
    "extra": {
        "wordpress-install-dir": "wp"
    },
    "repositories":[
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],
    "scripts": {
        "build-theme-assets": "cd wp-content/themes/paranges/src && parcel build js/main.js --out-dir dist --public-url ./"
     }
}
